
from tkinter import *

logs = Tk()
logs.title("test")
logs.geometry("1200x900")
logs.config(bg="#E6E6E6")

start=Frame(logs,bg="#E6E6E6")
start.pack()

# Images
karatuves = PhotoImage(file='C:\\Users\\PC\\Documents\\26\\Capture.png')
START_POGA = PhotoImage(file='C:\\Users\\PC\\Documents\\26\\START POGA.png')

class Start:
    def main_screen(self):
        main = Canvas(start, width=1200, height=900, bg="#E6E6E6", highlightthickness=0)
        main.pack()

        # start virsraksts
        main.create_text(600, 200, text="Karatuves", font=("MingLiU_HKSCS-ExtB", 60, "bold"))

        # bilde(karatuves)
        main.create_image(600, 450, image=karatuves)

        # bilde(start)
        main.create_image(600, 750, image=START_POGA)

        # start_poga
        self.Start_poga()

    def Start_poga(self):
        start_button = Button(start, text="START", font=("MingLiU_HKSCS-ExtB", 60, "bold"), bg="#D7D7D7", activebackground="#D7D7D7", relief="flat", width=10, height=1, command=self.start_action)

        start_button.place(x=385, y=685)

    def start_action(self):
        start.destroy()
#POGAAAAAAAM
        spele_app.poga_A()
        spele_app.poga_B()
        spele_app.poga_C()
        spele_app.poga_D()
        spele_app.poga_E()
        spele_app.poga_F()
        spele_app.poga_G()
        spele_app.poga_H()
        spele_app.poga_I()
        spele_app.poga_J()
        spele_app.poga_K()
        spele_app.poga_L()
        spele_app.poga_M()
        spele_app.poga_N()
        spele_app.poga_O()
        spele_app.poga_P()
        spele_app.poga_Q()
        spele_app.poga_R()
        spele_app.poga_S()
        spele_app.poga_T()
        spele_app.poga_U()
        spele_app.poga_V()
        spele_app.poga_W()
        spele_app.poga_X()
        spele_app.poga_Y()
        spele_app.poga_Z()

start_app = Start()
start_app.main_screen()

#SPÄ’LE



BURTU_SH = PhotoImage(file='C:\\Users\\PC\\Documents\\26\\Burti.png')


game = Frame(logs, bg="#E6E6E6")
game.pack()

class Spele_SCREEN():

    def speles_screen(self):
        speles_sh = Canvas(game, width=1200, height=900, bg="#E6E6E6", highlightthickness=0)
        speles_sh.pack()

        speles_sh.create_image(600, 650, image=BURTU_SH)

#BURTU POGAS AAAAAAAAAAAA

    def poga_A(self):
        poga_A = Button(game, text="A", fg="#858585", font=("Kozuka Gothic Pr6N B", 23, "bold"), bg="#D7D7D7",activebackground="#D7D7D7", relief="flat", width=2, height=1, command=self.poga_A_func)
        poga_A.place(x=180, y=530)

    def poga_A_func(self):
        print("A")

    def poga_B(self):
