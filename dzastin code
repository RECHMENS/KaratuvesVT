from tkinter import *



logs = Tk()
logs.title("test")
logs.geometry("1200x900")
logs.config(bg="#E6E6E6")

#Images

karatuves = PhotoImage(file='C:\\Users\\User\\Documents\\26\\Capture.png') #Dzastina path S202-8 FÄ“likssa path 202-04
START_POGA = PhotoImage(file='C:\\Users\\User\\Documents\\26\\START POGA.png')

def main_screen():
 main = Canvas(logs, width=1200, height=900, bg="#E6E6E6", highlightthickness= 0)
 main.pack()


#start virsraksts

 main.create_text(600,200, text = "Karatuves", font = ("MingLiU_HKSCS-ExtB", 60,"bold"))

#bilde(karatuves)

 main.create_image(600, 450, image=karatuves)

#bilde(start)

 main.create_image(600, 750, image=START_POGA)

#start_poga

 start = Canvas (logs, width = 435, height = 137,bg="#D7D7D7", highlightthickness= 0)
 start.place(x = 380, y = 685)

 start.create_text(220, 70, text = "START", font = ("MingLiU_HKSCS-ExtB", 60,"bold"))

main_screen()
logs.mainloop()
