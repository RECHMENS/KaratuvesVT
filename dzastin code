from tkinter import *



logs = Tk()
logs.title("test")
logs.geometry("1200x900")
logs.config(bg="#E6E6E6")
main = Canvas(logs, width=1200, height=900, bg="#E6E6E6", highlightthickness= 0)
main.pack()

#start

#start virsraksts

main.create_text(600,200, text = "Karatuves", font = ("MingLiU_HKSCS-ExtB", 60,"bold"))

#bilde

pakaries = PhotoImage(file='C:\\Users\\S202-8\\Documents\\26\\Capture.png') #Dzastina path S202-8 FÄ“likssa path 202-04
main.create_image(600, 450, image=pakaries)

#start poga 

START_POGA = PhotoImage(file='C:\\Users\\S202-8\\Documents\\26\\START POGA.png')
main.create_image(600, 750, image=START_POGA)
main.create_text(600,750, text = "START", font = ("MingLiU_HKSCS-ExtB", 60,"bold"))

start = Canvas (logs, width = 200, height = 100,bg="red")
start.place(x = 600, y = 750)

logs.mainloop()
